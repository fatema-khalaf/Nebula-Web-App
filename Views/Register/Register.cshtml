@{
    Layout = "_Register";
    @model Nebula.VM.RegisterVm;
}
<section class="main-container col2-right-layout" style="margin-left: 35%;margin-right: 35%;width: 30%;">
    <div class="main container" style="width: 100%;">
        <div class="row">
            <div class="col-main col-sm-9 animated" style="visibility: visible; width: 100%;">
                <div class="my-account">
                    <div class="page-title">
                        <h1>Register As Admin</h1>
                    </div>
                    <form action="#" method="post" id="form-validate">
                        <div class="fieldset group-select">
                            <input name="form_key" type="hidden" value="">
                            <input type="hidden" name="success_url" value="">
                            <input type="hidden" name="error_url" value="">
                            <h2 class="legend">Account Information</h2>
                            <ul class="">

                                <li class="">
                                    <div class="">
                                        <label for="company" asp-for="Email">Email<span class="required">*</span></label>
                                        <br>
                                        <input asp-for="Email" type="text" value="" class="input-text required-entry">
                                    </div>
                                </li>
                                <li class="">
                                    <div class="">
                                        <label for="company" asp-for="Password">Password<span class="required">*</span></label>
                                        <br>
                                        <input asp-for="Password" type="text" value="" class="input-text required-entry">
                                    </div>
                                </li>

                            </ul>
                        </div>
                        <div class="buttons-set">
                            <p class="required">* Required Fields</p>
                            <button type="submit" title="" class="button"><span>Create account </span></button>
                        </div>
                        <div class="buttons-set">
                            <a href="@Url.Action("Login",controller:"Register")"><small>>> </small>Already have an account? Log in</a>
                        </div>
                        <div class="buttons-set">
                            <a href="@Url.Action("Index",controller:"User")"><small>>> </small>Continue to the website</a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>

